{
    "manifest_version": 3,
    "name": "Mi Momentum",
    "description": "Base Level Extension",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["chrome://newtab"],
            "js": ["contentScript.js"]
        }
    ],

    "action": {
      "default_popup": "index.html",
      "default_icon": "hello_extensions.png",
      "permissions": ["tabs"]
    }
  }